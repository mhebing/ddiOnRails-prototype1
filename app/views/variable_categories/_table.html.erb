<table>
  <tr>
    <th>Value</th>
    <th>Label</th>
    <th>Frequency</th>
  </tr>

<% @variable_categories.each do |variable_category| %>
  <tr>
    <td><b><%= variable_category.value %></b></td>
    <td><%= variable_category.label %></td>
    <td><%= variable_category.frequency %></td>
  </tr>
<% end %>
</table>

